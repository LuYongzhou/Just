<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lu.payment.mapper.UserMapper">

    <insert id="insert" parameterType="com.lu.common.dto.UserInsertDto">
        insert into USER
        <trim suffix=")" prefix="(" suffixOverrides="," >
            <if test="id != null">
                TU_ID,
            </if>
            <if test="accessName != null and accessName !='' ">
                TU_ACCESS_NAME,
            </if>
            <if test="userName != null and userName !='' ">
                TU_USER_NAME,
            </if>
            <if test="describe != null and describe !='' ">
                TU_DESCRIBE,
            </if>
            <if test="mobileNo != null and mobileNo !='' ">
                TU_MOBILE_NO,
            </if>
            <if test="email != null and email !='' ">
                TU_E_MAIL,
            </if>
            <if test="password != null and password !='' ">
                TU_PASSWORD,
            </if>
            <if test="passwordEnable != null ">
                TU_PASSWORD_ENABLE,
            </if>
            <if test="remark != null and remark !='' ">
                TU_REMARK,
            </if>
            <if test="enable != null and enable !='' ">
                TU_ENABLE,
            </if>
            TU_CREATE_TIME,
            TU_UPDATE_TIME,
            <if test="operateUser != null and operateUser !='' ">
                TU_OPERATE_USER,
            </if>
            <if test="deleteStatus != null">
                DELETE_STATUS,
            </if>
        </trim>
        VALUES
        <trim suffix=")" prefix="(" suffixOverrides="," >
            <if test="id != null">
                #{id,jdbcType=BIGINT},
            </if>
            <if test="accessName != null and accessName !='' ">
                #{accessName,jdbcType=VARCHAR},
            </if>
            <if test="userName != null and userName !='' ">
                #{userName,jdbcType=VARCHAR},
            </if>
            <if test="describe != null and describe !='' ">
                #{describe,jdbcType=VARCHAR},
            </if>
            <if test="mobileNo != null and mobileNo !='' ">
                #{mobileNo,jdbcType=VARCHAR},
            </if>
            <if test="email != null and email !='' ">
                #{email,jdbcType=VARCHAR},
            </if>
            <if test="password != null and password !='' ">
                #{password,jdbcType=VARCHAR},
            </if>
            <if test="passwordEnable != null ">
                #{passwordEnable,jdbcType=INTEGER},
            </if>
            <if test="remark != null and remark !='' ">
                #{remark,jdbcType=VARCHAR},
            </if>
            <if test="enable != null ">
                #{enable,jdbcType=INTEGER},
            </if>
            NOW(),
            NOW(),
            <if test="operateUser != null and operateUser !='' ">
                #{operateUser,jdbcType=VARCHAR},
            </if>
            <if test="deleteStatus != null">
                #{deleteStatus,jdbcType=INTEGER},
            </if>
        </trim>

    </insert>
</mapper>